<?xml version="1.0" encoding="utf-8"?>
<com.android.keyguard.KeyguardSecSecurityContainer xmlns:android="http://schemas.android.com/apk/res/android" xmlns:androidprv="http://schemas.android.com/apk/res-auto"
    android:layout_gravity="center"
    android:id="@+id/keyguard_security_container"
    android:fitsSystemWindows="true"
    android:clipChildren="false"
    android:clipToPadding="false"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:importantForAccessibility="yes">
    <com.android.keyguard.KeyguardSecurityViewFlipper
        android:id="@+id/view_flipper"
        android:clipChildren="false"
        android:clipToPadding="false"
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        androidprv:layout_constraintEnd_toEndOf="parent"
        androidprv:layout_constraintStart_toStartOf="parent"/>
    <include layout="@layout/keyguard_punch_hole_vi_view"/>
    <include layout="@layout/keyguard_carrier_text_view"/>
    <include layout="@layout/keyguard_biometric_view"/>
    <include layout="@layout/keyguard_arrow_view"/>
</com.android.keyguard.KeyguardSecSecurityContainer>
