<?xml version="1.0" encoding="utf-8"?>
<com.android.systemui.qs.SecQuickStatusBarHeader xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_gravity="clip_vertical|right"
    android:id="@+id/header"
    android:background="@android:color/transparent"
    android:paddingTop="0dp"
    android:clickable="false"
    android:clipChildren="false"
    android:clipToPadding="false"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:baselineAligned="false"
    android:paddingStart="0dp"
    android:paddingEnd="0dp">
    <RelativeLayout
        android:layout_gravity="top"
        android:id="@+id/qs_container"
        android:clipChildren="false"
        android:clipToPadding="false"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <include layout="@layout/sec_qqs_date_buttons"/>
        <com.android.systemui.qs.SecQuickQSPanel
            android:id="@+id/quick_qs_panel"
            android:paddingBottom="0dp"
            android:focusable="true"
            android:clipChildren="false"
            android:clipToPadding="false"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="0dp"
            android:layout_below="@+id/quick_qs_date_buttons"
            android:importantForAccessibility="yes"
            android:accessibilityTraversalAfter="@+id/quick_qs_date_buttons"/>
    </RelativeLayout>
</com.android.systemui.qs.SecQuickStatusBarHeader>
