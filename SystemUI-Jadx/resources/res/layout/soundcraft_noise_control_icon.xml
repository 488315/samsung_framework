<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:gravity="center"
    android:layout_gravity="center"
    android:orientation="vertical"
    android:id="@+id/item_root"
    android:layout_width="@dimen/soundcraft_noise_effect_box_icon_long"
    android:layout_height="123dp">
    <ImageView
        android:layout_gravity="center"
        android:id="@+id/item_icon"
        android:background="@drawable/soundcraft_icon_background_unselected"
        android:layout_width="44dp"
        android:layout_height="44dp"
        android:src="@drawable/soundcraft_ic_buds3_anc_off_unselelcted"
        android:scaleType="centerInside"/>
    <TextView
        android:textSize="12sp"
        android:textColor="#fffafaff"
        android:layout_gravity="center"
        android:id="@+id/item_name"
        android:layout_width="@dimen/soundcraft_noise_effect_box_icon_long"
        android:layout_height="wrap_content"
        android:fontFamily="sec"
        android:textAlignment="center"
        android:paddingStart="2dp"
        android:paddingEnd="2dp"
        android:textFontWeight="400"/>
</LinearLayout>
