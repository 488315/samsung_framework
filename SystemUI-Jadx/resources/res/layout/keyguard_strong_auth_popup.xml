<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:gravity="center"
    android:background="@drawable/keyguard_strong_auth_timeout_popup_drawable"
    android:paddingLeft="@dimen/kg_strong_auth_timeout_popup_padding_left_right"
    android:paddingTop="@dimen/kg_strong_auth_timeout_popup_padding_top_bottom"
    android:paddingRight="@dimen/kg_strong_auth_timeout_popup_padding_left_right"
    android:paddingBottom="@dimen/kg_strong_auth_timeout_popup_padding_top_bottom"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">
    <TextView
        android:id="@+id/strong_auth_popup_message"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        style="@style/keyguard_strong_auth_popup_text_style"/>
</LinearLayout>
