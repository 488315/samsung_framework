<?xml version="1.0" encoding="utf-8"?>
<com.android.systemui.navigationbar.SamsungNavigationBarView xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/navigation_bar_view"
    android:background="@drawable/system_bar_background"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <com.android.systemui.navigationbar.SamsungNavigationBarSetupWizardView
        android:id="@+id/navigation_setupwizard"
        android:visibility="gone"
        android:layout_width="match_parent"
        android:layout_height="match_parent"/>
    <com.android.systemui.navigationbar.SamsungNavigationBarInflaterView
        android:id="@+id/navigation_inflater"
        android:layout_width="match_parent"
        android:layout_height="match_parent"/>
</com.android.systemui.navigationbar.SamsungNavigationBarView>
