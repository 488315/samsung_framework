<?xml version="1.0" encoding="utf-8"?>
<com.android.systemui.screenshot.sep.widget.SemScreenshotLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <ImageView
        android:id="@+id/white_bg"
        android:background="#99ffffff"
        android:visibility="gone"
        android:layout_width="match_parent"
        android:layout_height="match_parent"/>
    <com.android.systemui.screenshot.sep.ScreenshotSelectorView
        android:id="@+id/global_screenshot_selector"
        android:visibility="gone"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:pointerIcon="cell"/>
</com.android.systemui.screenshot.sep.widget.SemScreenshotLayout>
