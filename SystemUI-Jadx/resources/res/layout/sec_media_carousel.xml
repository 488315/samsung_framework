<?xml version="1.0" encoding="utf-8"?>
<com.android.systemui.media.QSMediaCornerRoundedView xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/media_carousel_layout"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <androidx.viewpager.widget.ViewPager
        android:id="@+id/sec_media_carousel_view_pager"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"/>
    <com.android.systemui.qs.SecPageIndicator
        android:layout_gravity="top|center_horizontal"
        android:id="@+id/sec_media_page_indicator"
        android:visibility="gone"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="10dp"/>
</com.android.systemui.media.QSMediaCornerRoundedView>
