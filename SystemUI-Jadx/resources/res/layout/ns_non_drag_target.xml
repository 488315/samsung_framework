<?xml version="1.0" encoding="utf-8"?>
<com.android.wm.shell.naturalswitching.NonDragTarget xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layoutDirection="ltr">
    <ImageView
        android:id="@+id/non_target"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"/>
    <ImageView
        android:id="@+id/non_target_blur"
        android:visibility="gone"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:alpha="0"/>
</com.android.wm.shell.naturalswitching.NonDragTarget>
