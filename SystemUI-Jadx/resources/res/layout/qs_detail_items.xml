<?xml version="1.0" encoding="utf-8"?>
<com.android.systemui.qs.QSDetailItems xmlns:android="http://schemas.android.com/apk/res/android" xmlns:sysui="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <com.android.systemui.qs.AutoSizingList
        android:orientation="vertical"
        android:id="@android:id/list"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        sysui:itemHeight="@dimen/qs_detail_item_height"
        style="@style/AutoSizingList"/>
    <LinearLayout
        android:orientation="vertical"
        android:id="@android:id/empty"
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <ImageView
            android:id="@android:id/icon"
            android:visibility="gone"
            android:layout_width="56dp"
            android:layout_height="56dp"
            android:tint="?android:attr/textColorSecondary"/>
        <TextView
            android:textAppearance="@style/TextAppearance.QS.DetailEmpty"
            android:id="@android:id/title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/qs_detail_item_icon_margin_from_label"/>
    </LinearLayout>
</com.android.systemui.qs.QSDetailItems>
