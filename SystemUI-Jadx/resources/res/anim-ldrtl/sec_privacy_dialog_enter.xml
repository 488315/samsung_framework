<?xml version="1.0" encoding="utf-8"?>
<set xmlns:android="http://schemas.android.com/apk/res/android" android:shareInterpolator="false">
    <scale
        android:interpolator="@anim/interpolator_privacy_dialog_enter_scale"
        android:duration="@integer/privacy_dialog_enter_scale_duration"
        android:pivotX="0%"
        android:pivotY="0%"
