<?xml version="1.0" encoding="utf-8"?>
<ripple xmlns:android="http://schemas.android.com/apk/res/android" android:color="@color/sec_qs_tiles_ripple_background">
    <item>
        <shape>
            <solid android:color="@color/qs_tile_container_bg"/>
            <corners android:radius="@dimen/panel_bar_corner_radius"/>
        </shape>
        <stroke
            android:width="1dp"
            android:color="#26ffffff"/>
    </item>
    <item android:id="@android:id/mask">
        <shape>
            <corners android:radius="22dp"/>
            <solid android:color="@android:color/white"/>
        </shape>
    </item>
</ripple>
