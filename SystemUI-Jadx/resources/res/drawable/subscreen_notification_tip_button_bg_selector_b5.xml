<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:state_focused="true"
        android:state_pressed="false">
        <layer-list>
            <item>
                <shape android:shape="rectangle">
                    <solid android:color="@color/subscreen_notification_tip_button_background_b5"/>
                    <corners android:radius="22dp"/>
                </shape>
            </item>
            <item>
                <shape android:shape="rectangle">
                    <solid android:color="@color/subscreen_notification_common_focused_color_b5"/>
                    <corners android:radius="22dp"/>
                </shape>
            </item>
        </layer-list>
    </item>
    <item android:drawable="@drawable/subscreen_notification_tip_button_background_b5"/>
</selector>
