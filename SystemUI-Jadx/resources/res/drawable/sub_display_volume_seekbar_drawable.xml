<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape android:shape="rectangle">
            <size android:height="@dimen/volume_sub_large_display_seek_bar_height"/>
            <corners android:radius="23dp"/>
            <solid android:color="@color/transparent"/>
            <stroke
                android:width="1dp"
                android:color="@color/sub_large_display_volume_seekbar_background_stroke_color_blur"/>
        </shape>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape android:shape="rectangle">
                <size android:height="@dimen/volume_sub_large_display_seek_bar_height"/>
                <corners android:radius="23dp"/>
                <solid android:color="@color/sub_large_display_volume_seekbar_progress_color"/>
                <stroke
                    android:width="1dp"
                    android:color="@color/sub_large_display_volume_seekbar_progress_color"/>
            </shape>
        </clip>
    </item>
</layer-list>
