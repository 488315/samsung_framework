<?xml version="1.0" encoding="utf-8"?>
<set xmlns:android="http://schemas.android.com/apk/res/android" android:shareInterpolator="false">
    <scale
        android:interpolator="@anim/interpolator_privacy_dialog_exit_scale"
        android:duration="@integer/privacy_dialog_exit_scale_duration"
        android:pivotX="100%"
        android:pivotY="0%"
        android:fromXScale="1"
        android:toXScale="0.8"
        android:fromYScale="1"
        android:toYScale="0.8"/>
    <alpha
        android:interpolator="@anim/interpolator_privacy_dialog_alpha"
        android:duration="@integer/privacy_dialog_alpha_duration"
        android:fromAlpha="1"
        android:toAlpha="0"/>
</set>
