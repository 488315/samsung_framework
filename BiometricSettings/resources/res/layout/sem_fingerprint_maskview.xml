<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/sem_fingerprint_maskview_layout"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <TextView
        android:textSize="40dp"
        android:textColor="#ff0000"
        android:id="@+id/sem_fingerprint_maskview_debug_textview"
        android:visibility="gone"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"/>
    <View
        android:id="@+id/sem_fingerprint_maskview"
        android:layout_width="match_parent"
        android:layout_height="match_parent"/>
    <com.samsung.android.biometrics.app.setting.fingerprint.LightSourceView
        android:id="@+id/sem_fingerprint_lightsource_surfaceview"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"/>
</FrameLayout>
