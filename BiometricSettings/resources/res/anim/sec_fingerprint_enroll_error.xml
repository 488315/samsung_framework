<?xml version="1.0" encoding="utf-8"?>
<set xmlns:android="http://schemas.android.com/apk/res/android" android:fillAfter="true">
    <scale
        android:interpolator="@interpolator/sec_sine_in_out_60"
        android:duration="150"
        android:pivotX="50%"
        android:pivotY="50%"
        android:fillAfter="true"
        android:fromXScale="0.95"
        android:toXScale="1"
        android:fromYScale="0.95"
        android:toYScale="1"/>
    <alpha
        android:interpolator="@interpolator/sec_sine_in_out_33"
        android:duration="150"
        android:fillAfter="true"
        android:fromAlpha="0"
        android:toAlpha="1"/>
</set>
